extends layout_adm

block content
  div(class="hero-image")
    div(class="hero-content")
      div(class="hero-content-inner")
        h1(class='homeTitle') MOTOSPOT

        div(class="listing")
          ul
            each val in list_biketypes
              li(class="linkedList")
                p TEST #{val.length}
                a(class="linkedText" href=val.adm_url) #{val.name}

            else
              li(class="emptyList") There are no biketype.